<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:themes="clr-namespace:Material.Styles.Themes;assembly=Material.Styles">

    <themes:MaterialTheme BaseTheme="Dark" PrimaryColor="DeepPurple" SecondaryColor="Lime" />
    <StyleInclude Source="Controls/Window.axaml" />
    <StyleInclude Source="Controls/Labels.axaml" />
    <StyleInclude Source="Controls/Borders.axaml" />
    <StyleInclude Source="Controls/Buttons.axaml" />
    <StyleInclude Source="Controls/TextBlocks.axaml" />
    <StyleInclude Source="Controls/TextBoxes.axaml" />
    <StyleInclude Source="Controls/Slider.axaml" />
    <Style Selector="ListBox">
        <Setter Property="Background" Value="{DynamicResource PanelsColor}" />
        <Setter Property="CornerRadius" Value="10" />
    </Style>
</Styles>