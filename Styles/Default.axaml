<Styles xmlns="https://github.com/avaloniaui"
        xmlns:themes="clr-namespace:ShadUI;assembly=ShadUI"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Design.PreviewWith>
        <Border Padding="20">
            <!-- Add Controls for Previewer Here -->
        </Border>
    </Design.PreviewWith>

    <themes:ShadTheme/>
    <Style Selector="ListBox">
        <Setter Property="Background" Value="#ac212121"/>
        <Setter Property="CornerRadius" Value="7"/>
        <Setter Property="Padding" Value="5"/>
        <Setter Property="Margin" Value="5"/>
    </Style>
        
    <Style Selector="ListBoxItem">
        <Setter Property="CornerRadius" Value="7"/>
        <Setter Property="BorderBrush" Value="LightGreen"/>
        <Setter Property="Background" Value="#33212121"/>
    </Style>
        
    <Style Selector="Button">
        <Setter Property="MaxHeight" Value="70"/>
        <Setter Property="Height" Value="60"/>
        <Setter Property="Width" Value="60"/>
        <Setter Property="BorderBrush" Value="AliceBlue"/>
        <Setter Property="BorderThickness" Value="2"/>
        <Setter Property="Background" Value="#4D2F4F4F"/>
        <Setter Property="CornerRadius" Value="7"/>
        <Setter Property="Padding" Value="12"/>
        <Setter Property="Margin" Value="2"/>
    </Style>
        
    <Style Selector="Button.h2">
        <Setter Property="MaxHeight" Value="90"/>
        <Setter Property="Width" Value="90"/>
        <Setter Property="CornerRadius" Value="7"/>
        <Setter Property="Margin" Value="2"/>
        <Setter Property="Background" Value="#33A9A9A9"/>
        <Setter Property="BorderBrush" Value="AliceBlue"/>
    </Style>
        
    <Style Selector="Border">
        <Setter Property="Background" Value="#33A9A9A9"/>
        <Setter Property="CornerRadius" Value="7"/>
        <Setter Property="Padding" Value="5"/>
        <Setter Property="Margin" Value="5"/>
        <Setter Property="BorderBrush" Value="AliceBlue"/>
    </Style>
</Styles>
